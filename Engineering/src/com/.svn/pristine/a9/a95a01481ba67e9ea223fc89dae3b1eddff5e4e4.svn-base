package com.firstplayable.hxlib.utils;

import com.firstplayable.hxlib.Debug;

class LambdaX extends Lambda
{
    /**
     * Shuffles the list.
     */
    //TODO: can we change this to use Iterables?
    //NOTE: Deprecating this in favor of hxlib.utils.ArrayTools
    public static function shuffle<T>( ar:Array<T> ):Void
    {
    	Debug.log('LambdaX.shuffle is Deprecated.  Please use ArrayTools.shuffle instead.');

    	com.firstplayable.hxlib.utils.ArrayTools.shuffle(ar);
    }
}