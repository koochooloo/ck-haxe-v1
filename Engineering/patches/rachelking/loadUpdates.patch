--- lib/find_assets.py	(nonexistent)
+++ lib/find_assets.py	(working copy)
@@ -0,0 +1,132 @@
+import json
+
+import os
+import os.path
+
+import sys
+
+TOP_MENU = "topMenu"
+PANEL = "panel"
+SPRITE_OBJECT = "spriteObject"
+BUTTON = "button"
+INHERITABLE = "inheritable"
+RESOURCE = "resource"
+
+ASSETS_PREFIX = "2d/"
+
+BUTTON_UP_SUFFIX = "_up"
+BUTTON_DOWN_SUFFIX = "_down"
+BUTTON_OVER_SUFFIX = "_over"
+BUTTON_DISABLED_SUFFIX = "_disabled"
+
+PNG_EXT = ".png"
+IFL_EXT = ".ifl"
+
+COMMA = ","
+
+NEWLINE = "\n"
+
+TP_SETTINGS = "DEFAULT"
+
+JOINER = "_and_"
+
+paist_mapping = {}
+
+def parse_json(sheet_id, json_obj):
+    if TOP_MENU in json_obj:
+        parse_top_menu(sheet_id, json_obj[TOP_MENU])
+
+def parse_top_menu(sheet_id, json_obj):
+    if PANEL in json_obj:
+        parse_panel(sheet_id, json_obj[PANEL])
+
+    if BUTTON in json_obj:
+        parse_button(sheet_id, json_obj[BUTTON])
+
+    if SPRITE_OBJECT in json_obj:
+        parse_sprite_object(sheet_id, json_obj[SPRITE_OBJECT])
+
+def parse_panel(sheet_id, json_obj):
+    for panel_json_obj in json_obj:
+        if BUTTON in panel_json_obj:
+            parse_button(sheet_id, panel_json_obj[BUTTON])
+
+        if SPRITE_OBJECT in panel_json_obj:
+            parse_sprite_object(sheet_id, panel_json_obj[SPRITE_OBJECT])
+
+def parse_button(sheet_id, json_obj):
+    for button_json_obj in json_obj:
+        suffixes = [BUTTON_UP_SUFFIX, BUTTON_DOWN_SUFFIX, BUTTON_OVER_SUFFIX, BUTTON_DISABLED_SUFFIX]
+
+        for suffix in suffixes:
+            asset_path = ASSETS_PREFIX + button_json_obj[INHERITABLE][RESOURCE] + suffix + PNG_EXT
+
+            if os.path.exists(asset_path):
+                if sheet_id in paist_mapping:
+                    paist_mapping[sheet_id].add(asset_path)
+                else:
+                    paist_mapping[sheet_id] = {asset_path}
+
+def parse_sprite_object(sheet_id, json_obj):
+    for sprite_object_json_obj in json_obj:
+        basename = ASSETS_PREFIX + sprite_object_json_obj[INHERITABLE][RESOURCE]
+        ifl_filename = basename + IFL_EXT
+        png_filename = basename + PNG_EXT
+
+        is_ifl = os.path.exists(ifl_filename)
+
+        asset_path = png_filename
+
+        if is_ifl:
+            asset_path = basename
+
+        if sheet_id in paist_mapping:
+            paist_mapping[sheet_id].add(asset_path)
+        else:
+            paist_mapping[sheet_id] = {asset_path}
+	
+	
+
+def merge_common_subsets(sheet_id_to_asset_set):
+    asset_to_sheet_id = {}
+
+    for sheet_id, asset_set in sheet_id_to_asset_set.items():
+        for asset in asset_set:
+            if asset in asset_to_sheet_id:
+                asset_to_sheet_id[asset].append(sheet_id)
+            else:
+                asset_to_sheet_id[asset] = [sheet_id]
+    
+    sheet_id_to_asset_list = {}
+
+    for asset, sheet_id_list in asset_to_sheet_id.items():
+        sorted_list = sorted(sheet_id_list)
+
+        combined_sheet_id = JOINER.join(sorted_list)
+
+        if combined_sheet_id in sheet_id_to_asset_list:
+            sheet_id_to_asset_list[combined_sheet_id].append(asset)
+        else:
+            sheet_id_to_asset_list[combined_sheet_id] = [asset]
+
+    return sheet_id_to_asset_list
+
+if __name__ == "__main__":
+    target_directory = sys.argv[1]
+    output_filename = sys.argv[2]
+
+    for directory_name, _, files in os.walk(target_directory):
+        for json_filename in files:
+            with open(os.path.join(directory_name, json_filename), 'r') as json_file:
+                sheet_id, _ = os.path.splitext(json_filename)
+
+                json_obj = json.load(json_file)
+
+                parse_json(sheet_id, json_obj)
+
+    paist_mapping = merge_common_subsets(paist_mapping)
+
+    with open(output_filename, 'w') as csv_file:
+        for sheet_id, assets in paist_mapping.items():
+            for asset_filename in assets:
+                csv_file.write(asset_filename + COMMA + TP_SETTINGS + COMMA + sheet_id + NEWLINE)
